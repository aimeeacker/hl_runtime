[Unit]
Description=VPIN Engine Service
After=network-online.target
Wants=network-online.target


[Service]
Type=simple
UMask=0022
WorkingDirectory=/home/aimee/hl_runtime
ExecStart=/home/aimee/hl_runtime/.venv/bin/python /home/aimee/hl_runtime/vpin_engine.py
KillSignal=SIGINT
Restart=always

NoNewPrivileges=true
PrivateTmp=true
ProtectHome=false
ProtectSystem=full
ReadWritePaths=/home/aimee/hl_runtime
SyslogIdentifier=vpin-engine
Slice=hl_runtime.slice

[Install]
WantedBy=default.target
